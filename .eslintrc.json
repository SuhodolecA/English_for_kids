{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "warn",
    "no-param-reassign": "off",
    "no-debugger": "warn",
    "no-unused-vars": "warn",
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "function-paren-newline": ["error", "multiline"]
  }
}
